{
  "name": "react-timezone-select",
  "version": "1.2.2",
  "description": "Usable, dynamic React Timezone Select",
  "scripts": {
    "start": "concurrently npm:start:example npm:build",
    "start:example": "cd example && npm start",
    "prepublish": "npm run build",
    "postpublish": "npm run build:example && npm run deploy",
    "build": "node build.cjs",
    "build:example": "cd example && npm run build",
    "deploy": "gh-pages -d example/build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci ",
    "pretty-quick": "pretty-quick --staged",
    "tsc": "tsc"
  },
  "author": "Nico Domino <yo@ndo.dev>",
  "homepage": "https://github.com/ndom91/react-timezone-select",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ndom91/react-timezone-select.git"
  },
  "bugs": {
    "url": "https://github.com/ndom91/react-timezone-select/issues"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "timezone",
    "select",
    "react-select"
  ],
  "files": [
    "dist/**/*"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "peerDependencies": {
    "react": "^17.0.1 || ^16",
    "react-dom": "^17.0.1 || ^16"
  },
  "dependencies": {
    "react-select": "^5.2.2",
    "spacetime": "^6.16.3",
    "timezone-soft": "^1.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "concurrently": "^7.0.0",
    "esbuild": "^0.14.11",
    "gh-pages": "^3.2.3",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "react": "^17",
    "react-dom": "^17",
    "simple-git-hooks": "^2.7.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run pretty-quick"
  }
}
